<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.view.SignUpController">
   <children>
      <GridPane vgap="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="50.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="昵称" />
            <Label text="密码" GridPane.rowIndex="1" />
            <Label text="确认密码" GridPane.rowIndex="2" />
            <Label text="个性签名" GridPane.rowIndex="3" />
            <Label text="年龄" GridPane.rowIndex="4" />
            <Label text="部门" GridPane.rowIndex="6" />
            <Label text="性别" GridPane.rowIndex="5" />
            <Label text="出生日期" GridPane.rowIndex="7" />
            <Label text="电话" GridPane.rowIndex="8" />
            <TextField fx:id="nickname" promptText="必填" GridPane.columnIndex="1" />
            <PasswordField fx:id="password" promptText="必填" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <PasswordField fx:id="passwordConfirm" promptText="必填" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="phoneNumber" GridPane.columnIndex="1" GridPane.rowIndex="8" />
            <DatePicker fx:id="birthday" GridPane.columnIndex="1" GridPane.rowIndex="7" />
            <TextField fx:id="department" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            <HBox spacing="30.0" GridPane.columnIndex="1" GridPane.rowIndex="5">
               <children>
                  <RadioButton mnemonicParsing="false" selected="true" text="MALE" GridPane.columnIndex="1" GridPane.rowIndex="5">
                     <toggleGroup>
                        <ToggleGroup fx:id="Gender" />
                     </toggleGroup>
                  </RadioButton>
                  <RadioButton mnemonicParsing="false" text="FEMALE" toggleGroup="$Gender" GridPane.columnIndex="1" GridPane.rowIndex="5">
                     <GridPane.margin>
                        <Insets />
                     </GridPane.margin>
                  </RadioButton>
               </children>
            </HBox>
            <TextField fx:id="age" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField fx:id="signature" promptText="I can see you" GridPane.columnIndex="1" GridPane.rowIndex="3" />
         </children>
      </GridPane>
      <Button layoutX="200.0" layoutY="475.0" mnemonicParsing="false" onAction="#handleOK" prefWidth="125.0" text="注册" AnchorPane.bottomAnchor="50.0" AnchorPane.rightAnchor="50.0" />
   </children>
</AnchorPane>
